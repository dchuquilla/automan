<p id="notice"><%= notice %></p>

<h1>Settings</h1>

<table>
  <thead>
    <tr>
      <th>Car age</th>
      <th>Km max</th>
      <th>Km min</th>
      <th>Month max</th>
      <th>Month min</th>
      <th>Car type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.car_age %></td>
        <td><%= setting.km_max %></td>
        <td><%= setting.km_min %></td>
        <td><%= setting.month_max %></td>
        <td><%= setting.month_min %></td>
        <td><%= setting.car_type %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
