<p id="notice"><%= notice %></p>

<h1>User Car Settings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_car_settings.each do |user_car_setting| %>
      <tr>
        <td><%= link_to 'Show', [, user_car_setting] %></td>
        <td><%= link_to 'Edit', edit__user_car_setting_path(user_car_setting) %></td>
        <td><%= link_to 'Destroy', [, user_car_setting], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Car Setting', new__user_car_setting_path %>
